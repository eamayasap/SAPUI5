{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "invoices-config1",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": [
        "fiori",
        "run"
      ],
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npx",
      "args": [
        "--open",
        "index.html"
      ],
      "windows": {
        "runtimeExecutable": "npx.cmd"
      },
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "outputCapture": "std",
      "env": {
        "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false",
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/invoices\"}",
        "FIORI_TOOLS_BACKEND_CONFIG": "[{\"path\":\"/V3\",\"destination\":\"Northwind\"}]"
      }
    },
    {
      "name": "invoices-Mock Server",
      "cwd": "${workspaceFolder}",
