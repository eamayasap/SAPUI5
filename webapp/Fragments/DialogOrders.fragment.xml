<!--Dialogo que abre una tabla al dar click en un regitrso de la tabla List Types-->
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" >
    <Dialog id="IDDialogOrdersDialog1" title="Dialog Orders" >
        <content>
            <Table id="IDDialogOrdersTable1" items="{jsonEmployees>Orders}" width="700px" >
                <columns>
                    <Column id="IDDialogOrdersColumn1">
                        <header > 
                            <Label id="IDDialogOrdersLabel1" text="{i18n>OrderID}" />
                        </header>
                    </Column>
                    <Column id="IDDialogOrdersColumn2">
                        <header > 
                            <Label id="IDDialogOrdersLabel2" text="{i18n>Freight}" />
                        </header>
                    </Column>
                    <Column id="IDDialogOrdersColumn3">
                        <header > 
                            <Label id="IDDialogOrdersLabel3" text="{i18n>ShipAddress}" />
                        </header>
                    </Column>
                </columns>
                <items>
                    <ColumnListItem id="IDDialogOrdersList1" >
                        <cells>
                            <Label id="IDDialogOrdersLabel4" text="{jsonEmployees>OrderID}" />
                            <Label id="IDDialogOrdersLabel5" text="{jsonEmployees>Freight}" />
                            <Label id="IDDialogOrdersLabel6" text="{jsonEmployees>ShipAddress}" />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
            </content>
        <endButton>
            <Button id="IDDialogOrdersButton1" text="{i18n>DialogOrdersButton1}" press=".onCloseDialogOrders" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>